<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - RikaWebApp</title>
    <script defer src="https://kit.fontawesome.com/41c8b474e2.js" crossorigin="anonymous"></script>
    <link rel="stylesheet" href="~/css/site.min.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/RikaWebApp.styles.css" asp-append-version="true" />
    <script defer src="~/js/site.js" asp-append-version="true"></script>
    <script defer src="~/js/communication/communication.js" asp-append-version="true"></script>
    <script defer src="~/js/product/product.js" asp-append-version="true"></script>
    <script defer src="~/js/auth/auth.js" asp-append-version="true"></script>
    <script defer src="~/js/order/order.js" asp-append-version="true"></script>
</head>
<body>
    <div class="wrapper">
        <header>

        </header>

        <div class="container">
            <main>

                @RenderBody()
            </main>
        </div>

        <footer>

        </footer>
    </div>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
